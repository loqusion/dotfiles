{
  "config": {
    "prefix": "cfg",
    "body": [
      "local ok, m = pcall(require, '$1')",
      "if not ok then",
      "  vim.notify(",
      "    string.format('Failed to load config %s: %s', require('utils.api.path').current_filename(true), m),",
      "    vim.log.levels.ERROR",
      "  )",
      "  return",
      "end",
      "",
      "M.$2 = m"
    ]
  }
}
